<h1>Listing mash_ups</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mash_ups.each do |mash_up| %>
      <tr>
        <td><%= link_to 'Show', mash_up %></td>
        <td><%= link_to 'Edit', edit_mash_up_path(mash_up) %></td>
        <td><%= link_to 'Destroy', mash_up, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mash up', new_mash_up_path %>
